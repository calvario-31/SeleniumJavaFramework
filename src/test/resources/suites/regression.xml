<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" verbose="2" time-out="60000">
    <groups>
        <run>
            <include name="Regression"/>
        </run>
    </groups>

    <listeners>
        <listener class-name="listeners.TestListeners"/>
        <listener class-name="listeners.SuiteListeners"/>
        <listener class-name="listeners.InvokeMethodListeners"/>
    </listeners>

    <test name="credentials">
        <packages>
            <package name="org.complete.framework.credentials"/>
        </packages>
    </test>

    <test name="bars">
        <packages>
            <package name="org.complete.framework.bars"/>
        </packages>
    </test>

    <test name="shopping">
        <packages>
            <package name="org.complete.framework.shopping"/>
        </packages>
    </test>
</suite>